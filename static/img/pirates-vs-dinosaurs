<script language='Javascript'>
function GetMiniGallery( imageid, objectid, objecttype, dir )
{
	$('minigallery_status').set('html', '<center>Loading...<img src="//cf.geekdo-static.com/images/progress.gif"></center>');
	new Request.GEEK( 
		{
			url: '/geekimage.php', 
			data: 'action=minigallery&ajax=1&imageid='+imageid+'&objectid='+objectid+'&objecttype='+objecttype+'&dir='+dir,
			method: 'get',
			update: $('minigallery')
		} ).send();
}
</script>
